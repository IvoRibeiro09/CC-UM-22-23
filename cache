import java.io.File;  // Import the File class
import java.io.FileNotFoundException;  // Import this class to handle errors
import java.util.*;

public class cache {
    
private String default;    
private int ttl;
private String soasp;
private String soaadmin;
private String soaserial;
private String soarefresh;
private String soaretry;
private String soaexpire;
private List<String> ns;
private String smaller;
private List<String> mx;
private List<String> mxprio;
private HashMap<String,String> ips;
private List<String> www;
private List<String> wwwprio;
private List<String,String> names;


public setdefault(String s){
    this.default=s;
}

public setttl(int n){
    this.ttl=n;
}

public setsoasp(String s){
    this.soasp=s;
}

public setsoaadmin(String s){
    this.soaadmin=s;
}

public setsoaserial(String s){
    this.soaserial=s;
}

public setsoarefresh(String s){
    this.soarefresh=s;
}

public setsoaretry(String s){
    this.soaretry=s;
}

public setsoaexpire(String s){
    this.soasp=s;
}

public addnslist(String s){
    this.ns.add(s);
}

public setsmaller(String s){
    this.smaller=s;
}

public addmxlist(String s){
    this.mx.add(s);
}

public addmxpriolist(String s){
    this.mxprio.add(s);
}

public addipsmap(String tipo,String ip){
    this.ips.put(tipo,ip);
}

public addwwwlist(String s){
    this.www.add(s);
}

public addwwwpriolist(String s){
    this.wwwprio.add(s)
}

public addnamesmap(String tipo,String nome){
    this.names.put(tipo,nome);
}


    private static parser(String[] args){
        try {
            File ficheiro = new File("teste.txt");
            Scanner myReader = new Scanner(ficheiro);
            while (myReader.hasNextLine()) {
              String data = myReader.nextLine();
              String[] linha = data.split(" ");
              if (linha[1] != "#"){
                if(linha[1]=="TTL") {setttl(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="DEFAULT") {setdefault(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="SOASP") {setsoasp(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="SOAADMIN") {setsoaadmin(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="SOASERIAL") {setsoaserial(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="SOAREFRESH") {setsoarefresh(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="SOARETRY") {setsoaretry(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="SOAEXPIRE") {setsoaexpire(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="NS") {addnslist(linha[3])}
                else if(linha[1]=="Smaller.@") {setsmaller(linha[3])}
                else if(linha[1]=="@"&&linha[2]=="MX") {
                    addmxlist(linha[3]);
                    addmxpriolist(linha[4]);
                }
                else if(linha[2]=="A"&&linha[1]!="www") {addipsmap(linha[1],linha[3])}
                else if(linha[2]=="A"&&linha[1]=="www") {
                    addwwwlist(linha[3]);
                    addwwwpriolist(linha[4]);
                }
                else if(linha[2]=="CNAME") {addnamesmap(linha[1],linha[3])}
              }
            }
        } catch (Exception e) {
            System.out.println("Something went wrong.");
    }
}
}
